;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ РАЗЛИЧНЫЕ
	Сегменты.Номенклатура КАК Номенклатура,
	Сегменты.Характеристика КАК Характеристика,
	ИСТИНА КАК ИспользуетсяОтборПоСегментуНоменклатуры
ПОМЕСТИТЬ ОтборПоСегментуНоменклатуры
ИЗ
	РегистрСведений.НоменклатураСегмента КАК Сегменты
{ГДЕ
	Сегменты.Сегмент.* КАК СегментНоменклатуры,
	Сегменты.Номенклатура.* КАК Номенклатура,
	Сегменты.Характеристика.* КАК Характеристика}

ИНДЕКСИРОВАТЬ ПО
	Номенклатура,
	Характеристика,
	ИспользуетсяОтборПоСегментуНоменклатуры
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВЫБОР &amp;Периодичность
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Неделя)
			ТОГДА ПланыПродажОбороты.ПериодНеделя
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Декада)
			ТОГДА ПланыПродажОбороты.ПериодДекада
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Месяц)
			ТОГДА ПланыПродажОбороты.ПериодМесяц
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Квартал)
			ТОГДА ПланыПродажОбороты.ПериодКвартал
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Полугодие)
			ТОГДА ПланыПродажОбороты.ПериодПолугодие
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Год)
			ТОГДА ПланыПродажОбороты.ПериодГод
		ИНАЧЕ ПланыПродажОбороты.ПериодДень
	КОНЕЦ КАК ДинамическийПериод,
	ПланыПродажОбороты.Склад КАК Склад,
	ПланыПродажОбороты.Номенклатура КАК Номенклатура,
	ПланыПродажОбороты.Характеристика КАК Характеристика,
	СУММА(ПланыПродажОбороты.КоличествоОборот) КАК Количество,
	СУММА(ПланыПродажОбороты.СуммаОборот) КАК Сумма
ПОМЕСТИТЬ ПланПродаж
{ВЫБРАТЬ
	Склад.*}
ИЗ
	РегистрНакопления.ПланыПродаж.Обороты(, , Авто, Сценарий = &amp;Сценарий {(Подразделение), ((Номенклатура, Характеристика) В
			    (ВЫБРАТЬ
			        ОтборПоСегментуНоменклатуры.Номенклатура,
			        ОтборПоСегментуНоменклатуры.Характеристика
			    ИЗ
			        ОтборПоСегментуНоменклатуры КАК ОтборПоСегментуНоменклатуры
			    ГДЕ
			        ОтборПоСегментуНоменклатуры.ИспользуетсяОтборПоСегментуНоменклатуры = &amp;ИспользуетсяОтборПоСегментуНоменклатуры)), (Партнер В
			    (ВЫБРАТЬ
			        ОтборПоСегментуПартнеров.Партнер
			    ИЗ
			        ОтборПоСегментуПартнеров КАК ОтборПоСегментуПартнеров
			    ГДЕ
			        ОтборПоСегментуПартнеров.ИспользуетсяОтборПоСегментуПартнеров = &amp;ИспользуетсяОтборПоСегментуПартнеров))}) КАК ПланыПродажОбороты

СГРУППИРОВАТЬ ПО
	ПланыПродажОбороты.Номенклатура,
	ПланыПродажОбороты.Характеристика,
	ПланыПродажОбороты.Склад,
	ВЫБОР &amp;Периодичность
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Неделя)
			ТОГДА ПланыПродажОбороты.ПериодНеделя
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Декада)
			ТОГДА ПланыПродажОбороты.ПериодДекада
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Месяц)
			ТОГДА ПланыПродажОбороты.ПериодМесяц
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Квартал)
			ТОГДА ПланыПродажОбороты.ПериодКвартал
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Полугодие)
			ТОГДА ПланыПродажОбороты.ПериодПолугодие
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Год)
			ТОГДА ПланыПродажОбороты.ПериодГод
		ИНАЧЕ ПланыПродажОбороты.ПериодДень
	КОНЕЦ

ИНДЕКСИРОВАТЬ ПО
	ДинамическийПериод,
	Склад,
	Номенклатура,
	Характеристика
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	&amp;НачалоПериода КАК Период,
	КурсыВалютСрезПоследних.Валюта,
	КурсыВалютСрезПоследних.Курс,
	КурсыВалютСрезПоследних.Кратность
ПОМЕСТИТЬ КурсыВалют
ИЗ
	РегистрСведений.КурсыВалют.СрезПоследних(&amp;НачалоПериода, Валюта В (&amp;ВалютаУпр, &amp;ВалютаСценария)) КАК КурсыВалютСрезПоследних
ГДЕ
	&amp;ВалютаУпр &lt;&gt; &amp;ВалютаСценария

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	КурсыВалют.Период,
	КурсыВалют.Валюта,
	КурсыВалют.Курс,
	КурсыВалют.Кратность
ИЗ
	РегистрСведений.КурсыВалют КАК КурсыВалют
ГДЕ
	&amp;ВалютаУпр &lt;&gt; &amp;ВалютаСценария
	И КурсыВалют.Период &gt; &amp;НачалоПериода
	И КурсыВалют.Период &lt;= &amp;КонецПериода
	И КурсыВалют.Валюта В (&amp;ВалютаУпр, &amp;ВалютаСценария)

ИНДЕКСИРОВАТЬ ПО
	Период,
	КурсыВалютСрезПоследних.Валюта
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВЫБОР &amp;Периодичность
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Неделя)
			ТОГДА ВыручкаИСебестоимостьПродажОбороты.ПериодНеделя
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Декада)
			ТОГДА ВыручкаИСебестоимостьПродажОбороты.ПериодДекада
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Месяц)
			ТОГДА ВыручкаИСебестоимостьПродажОбороты.ПериодМесяц
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Квартал)
			ТОГДА ВыручкаИСебестоимостьПродажОбороты.ПериодКвартал
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Полугодие)
			ТОГДА ВыручкаИСебестоимостьПродажОбороты.ПериодПолугодие
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Год)
			ТОГДА ВыручкаИСебестоимостьПродажОбороты.ПериодГод
		ИНАЧЕ ВыручкаИСебестоимостьПродажОбороты.ПериодДень
	КОНЕЦ КАК ДинамическийПериод,
	АналитикаНоменклатуры.Склад КАК Склад,
	АналитикаНоменклатуры.Номенклатура КАК Номенклатура,
	АналитикаНоменклатуры.Характеристика КАК Характеристика,
	СУММА(ВыручкаИСебестоимостьПродажОбороты.КоличествоОборот) КАК ФактКоличество,
	СУММА(ВЫРАЗИТЬ(ВыручкаИСебестоимостьПродажОбороты.СуммаВыручкиОборот * ВЫБОР
				КОГДА &amp;ВалютаУпр &lt;&gt; &amp;ВалютаСценария
					ТОГДА ВЫБОР
							КОГДА ЕСТЬNULL(КурсыВалютыУпр.Кратность, ЕСТЬNULL(КурсыВалютыУпрНаНачало.Кратность, 0)) &gt; 0
									И ЕСТЬNULL(КурсыВалютыУпр.Курс, ЕСТЬNULL(КурсыВалютыУпрНаНачало.Курс, 0)) &gt; 0
									И ЕСТЬNULL(КурсыВалюты.Кратность, ЕСТЬNULL(КурсыВалютыНаНачало.Кратность, 0)) &gt; 0
									И ЕСТЬNULL(КурсыВалюты.Курс, ЕСТЬNULL(КурсыВалютыНаНачало.Курс, 0)) &gt; 0
								ТОГДА ЕСТЬNULL(КурсыВалютыУпр.Курс, ЕСТЬNULL(КурсыВалютыУпрНаНачало.Курс, 0)) * ЕСТЬNULL(КурсыВалюты.Кратность, ЕСТЬNULL(КурсыВалютыНаНачало.Кратность, 0)) / (ЕСТЬNULL(КурсыВалюты.Курс, ЕСТЬNULL(КурсыВалютыНаНачало.Курс, 0)) * ЕСТЬNULL(КурсыВалютыУпр.Кратность, ЕСТЬNULL(КурсыВалютыУпрНаНачало.Кратность, 0)))
							ИНАЧЕ 0
						КОНЕЦ
				ИНАЧЕ 1
			КОНЕЦ КАК ЧИСЛО(15, 2))) КАК ФактСумма
ПОМЕСТИТЬ Продажи
ИЗ
	РегистрНакопления.ВыручкаИСебестоимостьПродаж.Обороты(
			,
			,
			Авто,
			АналитикаУчетаНоменклатуры В
				(ВЫБРАТЬ РАЗЛИЧНЫЕ
					АналитикаУчетаНоменклатуры.КлючАналитики
				ИЗ
					ПланПродаж КАК ПланПродаж
						ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрСведений.АналитикаУчетаНоменклатуры КАК АналитикаУчетаНоменклатуры
						ПО
							ПланПродаж.Номенклатура = АналитикаУчетаНоменклатуры.Номенклатура
								И ПланПродаж.Характеристика = АналитикаУчетаНоменклатуры.Характеристика) {(Подразделение)}) КАК ВыручкаИСебестоимостьПродажОбороты
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.АналитикаУчетаНоменклатуры КАК АналитикаНоменклатуры
		ПО ВыручкаИСебестоимостьПродажОбороты.АналитикаУчетаНоменклатуры = АналитикаНоменклатуры.КлючАналитики
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.АналитикаУчетаПоПартнерам КАК АналитикаПоПартнерам
		ПО ВыручкаИСебестоимостьПродажОбороты.АналитикаУчетаПоПартнерам = АналитикаПоПартнерам.КлючАналитики}
		ЛЕВОЕ СОЕДИНЕНИЕ КурсыВалют КАК КурсыВалютыУпр
		ПО (&amp;ВалютаУпр &lt;&gt; &amp;ВалютаСценария)
			И (КурсыВалютыУпр.Валюта = &amp;ВалютаУпр)
			И ВыручкаИСебестоимостьПродажОбороты.ПериодДень = КурсыВалютыУпр.Период
		ЛЕВОЕ СОЕДИНЕНИЕ КурсыВалют КАК КурсыВалюты
		ПО (&amp;ВалютаУпр &lt;&gt; &amp;ВалютаСценария)
			И (КурсыВалюты.Валюта = &amp;ВалютаСценария)
			И ВыручкаИСебестоимостьПродажОбороты.ПериодДень = КурсыВалюты.Период
		ЛЕВОЕ СОЕДИНЕНИЕ КурсыВалют КАК КурсыВалютыУпрНаНачало
		ПО (&amp;ВалютаУпр &lt;&gt; &amp;ВалютаСценария)
			И (КурсыВалютыУпрНаНачало.Валюта = &amp;ВалютаУпр)
			И (КурсыВалютыУпрНаНачало.Период = &amp;НачалоПериода)
		ЛЕВОЕ СОЕДИНЕНИЕ КурсыВалют КАК КурсыВалютыНаНачало
		ПО (&amp;ВалютаУпр &lt;&gt; &amp;ВалютаСценария)
			И (КурсыВалютыНаНачало.Валюта = &amp;ВалютаСценария)
			И (КурсыВалютыНаНачало.Период = &amp;НачалоПериода)
{ГДЕ
	АналитикаПоПартнерам.Организация.*,
	АналитикаНоменклатуры.Номенклатура.*,
	(АналитикаПоПартнерам.Партнер В
			(ВЫБРАТЬ
				ОтборПоСегментуПартнеров.Партнер
			ИЗ
				ОтборПоСегментуПартнеров КАК ОтборПоСегментуПартнеров
			ГДЕ
				ОтборПоСегментуПартнеров.ИспользуетсяОтборПоСегментуПартнеров = &amp;ИспользуетсяОтборПоСегментуПартнеров))}

СГРУППИРОВАТЬ ПО
	АналитикаНоменклатуры.Склад,
	АналитикаНоменклатуры.Характеристика,
	АналитикаНоменклатуры.Номенклатура,
	ВЫБОР &amp;Периодичность
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Неделя)
			ТОГДА ВыручкаИСебестоимостьПродажОбороты.ПериодНеделя
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Декада)
			ТОГДА ВыручкаИСебестоимостьПродажОбороты.ПериодДекада
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Месяц)
			ТОГДА ВыручкаИСебестоимостьПродажОбороты.ПериодМесяц
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Квартал)
			ТОГДА ВыручкаИСебестоимостьПродажОбороты.ПериодКвартал
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Полугодие)
			ТОГДА ВыручкаИСебестоимостьПродажОбороты.ПериодПолугодие
		КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Год)
			ТОГДА ВыручкаИСебестоимостьПродажОбороты.ПериодГод
		ИНАЧЕ ВыручкаИСебестоимостьПродажОбороты.ПериодДень
	КОНЕЦ

ИНДЕКСИРОВАТЬ ПО
	ДинамическийПериод,
	Склад,
	Номенклатура,
	Характеристика
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	Таблица.Номенклатура   КАК Номенклатура,
	Таблица.Характеристика КАК Характеристика
ПОМЕСТИТЬ ВтНоменклатураПлана
ИЗ
	ПланПродаж КАК Таблица
ИНДЕКСИРОВАТЬ ПО
	Номенклатура, Характеристика
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВЫБОР &amp;Периодичность
			КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Неделя) ТОГДА
				НАЧАЛОПЕРИОДА(Заказы.ДатаОтгрузки, НЕДЕЛЯ)
			КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Декада) ТОГДА
				НАЧАЛОПЕРИОДА(Заказы.ДатаОтгрузки, ДЕКАДА)
			КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Месяц) ТОГДА
				НАЧАЛОПЕРИОДА(Заказы.ДатаОтгрузки, МЕСЯЦ)
			КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Квартал) ТОГДА
				НАЧАЛОПЕРИОДА(Заказы.ДатаОтгрузки, КВАРТАЛ)
			КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Полугодие) ТОГДА
				НАЧАЛОПЕРИОДА(Заказы.ДатаОтгрузки, ПОЛУГОДИЕ)
			КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Год) ТОГДА
				НАЧАЛОПЕРИОДА(Заказы.ДатаОтгрузки, ГОД)
			ИНАЧЕ
				НАЧАЛОПЕРИОДА(Заказы.ДатаОтгрузки, ДЕНЬ)
		КОНЕЦ                КАК ДинамическийПериод,

	Заказы.Склад             КАК Склад,
	Заказы.Номенклатура      КАК Номенклатура,
	Заказы.Характеристика    КАК Характеристика,
	
	СУММА(ВЫБОР КОГДА Заказы.ВариантОбеспечения В(
			ЗНАЧЕНИЕ(Перечисление.ВариантыОбеспечения.Отгрузить),
			ЗНАЧЕНИЕ(Перечисление.ВариантыОбеспечения.ОтгрузитьОбособленно)) ТОГДА

				ЕСТЬNULL(ЗаказыКОформлению.КОформлениюОстаток, 0)
			ИНАЧЕ
				Заказы.Количество
		КОНЕЦ)               КАК ЗаказаноКоличество

ПОМЕСТИТЬ Заказы
ИЗ
	Документ.ЗаказКлиента.Товары КАК Заказы

	ВНУТРЕННЕЕ СОЕДИНЕНИЕ ВтНоменклатураПлана КАК ОтборНоменклатуры
	ПО Заказы.Номенклатура   = ОтборНоменклатуры.Номенклатура
	 И Заказы.Характеристика = ОтборНоменклатуры.Характеристика

	ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ЗаказКлиента КАК РеквизитыЗаказа
	ПО Заказы.Ссылка = РеквизитыЗаказа.Ссылка

	ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ЗаказыКлиентов.Остатки(,
		ЗаказКлиента ССЫЛКА Документ.ЗаказКлиента
		И (Номенклатура, Характеристика) В(
			ВЫБРАТЬ
				ОтборНоменклатуры.Номенклатура   КАК Номенклатура,
				ОтборНоменклатуры.Характеристика КАК Характеристика
			ИЗ
				ВтНоменклатураПлана КАК ОтборНоменклатуры)) КАК ЗаказыКОформлению
	ПО Заказы.Ссылка   = ЗаказыКОформлению.ЗаказКлиента
	И Заказы.КодСтроки = ЗаказыКОформлению.КодСтроки

ГДЕ
	НЕ Заказы.Отменено
	И Заказы.Ссылка.Проведен
{ГДЕ
	РеквизитыЗаказа.Организация.*   КАК Организация,
	РеквизитыЗаказа.Подразделение.* КАК Подразделение,
	РеквизитыЗаказа.Партнер В(
		ВЫБРАТЬ
			ОтборПоСегментуПартнеров.Партнер
		ИЗ
			ОтборПоСегментуПартнеров КАК ОтборПоСегментуПартнеров
		ГДЕ
			ОтборПоСегментуПартнеров.ИспользуетсяОтборПоСегментуПартнеров = &amp;ИспользуетсяОтборПоСегментуПартнеров)}

СГРУППИРОВАТЬ ПО
	Заказы.Номенклатура, Заказы.Характеристика, Заказы.Склад,
	ВЫБОР &amp;Периодичность
			КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Неделя) ТОГДА
				НАЧАЛОПЕРИОДА(Заказы.ДатаОтгрузки, НЕДЕЛЯ)
			КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Декада) ТОГДА
				НАЧАЛОПЕРИОДА(Заказы.ДатаОтгрузки, ДЕКАДА)
			КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Месяц) ТОГДА
				НАЧАЛОПЕРИОДА(Заказы.ДатаОтгрузки, МЕСЯЦ)
			КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Квартал) ТОГДА
				НАЧАЛОПЕРИОДА(Заказы.ДатаОтгрузки, КВАРТАЛ)
			КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Полугодие) ТОГДА
				НАЧАЛОПЕРИОДА(Заказы.ДатаОтгрузки, ПОЛУГОДИЕ)
			КОГДА ЗНАЧЕНИЕ(Перечисление.Периодичность.Год) ТОГДА
				НАЧАЛОПЕРИОДА(Заказы.ДатаОтгрузки, ГОД)
			ИНАЧЕ
				НАЧАЛОПЕРИОДА(Заказы.ДатаОтгрузки, ДЕНЬ)
		КОНЕЦ

ИНДЕКСИРОВАТЬ ПО
	ДинамическийПериод,
	Склад, Номенклатура, Характеристика
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВложенныйЗапрос.ДинамическийПериод,
	ВложенныйЗапрос.Склад,
	ВложенныйЗапрос.Номенклатура,
	ВложенныйЗапрос.Характеристика,
	СУММА(ВложенныйЗапрос.ПланКоличество) КАК ПланКоличество,
	СУММА(ВложенныйЗапрос.ПланСумма) КАК ПланСумма,
	СУММА(ВложенныйЗапрос.ФактКоличество) КАК ФактКоличество,
	СУММА(ВложенныйЗапрос.ФактСумма) КАК ФактСумма,
	СУММА(ВложенныйЗапрос.ЗаказаноКоличество) КАК ЗаказаноКоличество,
	МАКСИМУМ(ВложенныйЗапрос.СкладПлан) КАК СкладПлан
ПОМЕСТИТЬ ПланПродажФакт
ИЗ
	(ВЫБРАТЬ
		ПланПродаж.ДинамическийПериод КАК ДинамическийПериод,
		ПланПродаж.Склад КАК Склад,
