	РегистрСведений.НоменклатураСегмента КАК Сегменты
{ГДЕ
	Сегменты.Сегмент.* КАК СегментНоменклатуры,
	Сегменты.Номенклатура.* КАК Номенклатура,
	Сегменты.Характеристика.* КАК Характеристика}

ИНДЕКСИРОВАТЬ ПО
	Сегменты.Номенклатура,
	Сегменты.Характеристика,
	ИспользуетсяОтборПоСегментуНоменклатуры
;
////////////////////////////////////////////////////////////////////////////////
// Построение отчета
//
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Набор.Номенклатура                              КАК Номенклатура,
	Набор.Номенклатура.ЕдиницаИзмерения             КАК ЕдиницаХранения,
	Набор.Номенклатура.ЕдиницаДляОтчетов            КАК ЕдиницаДляОтчетов,
	Набор.Номенклатура.КоэффициентЕдиницыДляОтчетов КАК КоэффициентЕдиницыДляОтчетов,
	&amp;ТекстЗапросаВес         КАК НоменклатураВес,
	&amp;ТекстЗапросаОбъем       КАК НоменклатураОбъем,
	Набор.Характеристика            КАК Характеристика,
	Набор.Склад                     КАК Склад,
	Набор.ДокументОснование         КАК ДокументОснование,

	СУММА(Набор.ВНаличииДляПродажи) КАК ВНаличииДляПродажи,
	СУММА(Набор.Резерв)             КАК Резерв,
	СУММА(Набор.СвободноДляПродажи) КАК СвободноДляПродажи,
	СУММА(Набор.ВНаличииНаСкладе)   КАК ВНаличииНаСкладе,
	СУММА(Набор.ОбособленныйРезерв) КАК ОбособленныйРезерв,
	СУММА(Набор.Отгружается)        КАК Отгружается

ИЗ(
	ВЫБРАТЬ
		Т.Номенклатура              КАК Номенклатура,
		Т.Характеристика            КАК Характеристика,
		Т.Склад                     КАК Склад,
		NULL                        КАК ДокументОснование,

		Т.ВНаличииОстаток           КАК ВНаличииДляПродажи,
		0                           КАК Резерв,
		Т.ВНаличииОстаток           КАК СвободноДляПродажи,
		0                           КАК ВНаличииНаСкладе,
		0                           КАК ОбособленныйРезерв,
		0                           КАК Отгружается
	ИЗ
		РегистрНакопления.СвободныеОстатки.Остатки( , {
			(Номенклатура, Характеристика) В(
				ВЫБРАТЬ
					ТаблицаОтбора.Номенклатура    КАК Номенклатура,
					ТаблицаОтбора.Характеристика  КАК Характеристика
				ИЗ
					ОтборПоСегментуНоменклатуры КАК ТаблицаОтбора
				ГДЕ
					ТаблицаОтбора.ИспользуетсяОтборПоСегментуНоменклатуры = &amp;ИспользуетсяОтборПоСегментуНоменклатуры)
			} {Склад.*, Номенклатура.*, Характеристика.*}) КАК Т

	ОБЪЕДИНИТЬ ВСЕ

	ВЫБРАТЬ
		Т.Номенклатура              КАК Номенклатура,
		Т.Характеристика            КАК Характеристика,
		Т.Склад                     КАК Склад,
		Т.Назначение.Заказ          КАК ДокументОснование,

		0                           КАК ВНаличииДляПродажи,
		0                           КАК Резерв,
		- Т.НаличиеПодЗаказОстаток  КАК СвободноДляПродажи,
		0                           КАК ВНаличииНаСкладе,
		Т.НаличиеПодЗаказОстаток    КАК ОбособленныйРезерв,
		0                           КАК Отгружается
	ИЗ
		РегистрНакопления.ОбеспечениеЗаказов.Остатки( , {
			(Номенклатура, Характеристика) В(
				ВЫБРАТЬ
					ТаблицаОтбора.Номенклатура    КАК Номенклатура,
					ТаблицаОтбора.Характеристика  КАК Характеристика
				ИЗ
					ОтборПоСегментуНоменклатуры КАК ТаблицаОтбора
				ГДЕ
					ТаблицаОтбора.ИспользуетсяОтборПоСегментуНоменклатуры = &amp;ИспользуетсяОтборПоСегментуНоменклатуры)
			}	 {Склад.*, Номенклатура.*, Характеристика.*}) КАК Т
	ГДЕ
		Т.НаличиеПодЗаказОстаток &gt; 0

	ОБЪЕДИНИТЬ ВСЕ

	ВЫБРАТЬ
		Т.Номенклатура              КАК Номенклатура,
		Т.Характеристика            КАК Характеристика,
		Т.Склад                     КАК Склад,
		NULL                        КАК ДокументОснование,

		0                           КАК ВНаличииДляПродажи,
		- Т.Количество              КАК Резерв,
		Т.Количество                КАК СвободноДляПродажи,
		0                           КАК ВНаличииНаСкладе,
		0                           КАК ОбособленныйРезерв,
		0                           КАК Отгружается

	ИЗ
		РегистрСведений.ДоступныеОстаткиПланируемыхПоступлений КАК Т

	{ГДЕ
		(Т.Номенклатура, Т.Характеристика) В(
			ВЫБРАТЬ
				ТаблицаОтбора.Номенклатура    КАК Номенклатура,
				ТаблицаОтбора.Характеристика  КАК Характеристика
			ИЗ
				ОтборПоСегментуНоменклатуры КАК ТаблицаОтбора
			ГДЕ
				ТаблицаОтбора.ИспользуетсяОтборПоСегментуНоменклатуры = &amp;ИспользуетсяОтборПоСегментуНоменклатуры)}

	{ГДЕ
		Т.Склад.*          КАК Склад,
		Т.Номенклатура.*   КАК Номенклатура,
		Т.Характеристика.* КАК Характеристика
	}
	ГДЕ
		Т.ДатаДоступности = ДАТАВРЕМЯ(1, 1, 1)
		И Т.Количество &lt; 0

	ОБЪЕДИНИТЬ ВСЕ
	
	ВЫБРАТЬ
		Т.Номенклатура              КАК Номенклатура,
		Т.Характеристика            КАК Характеристика,
		Т.Склад                     КАК Склад,
		Т.ДокументОтгрузки          КАК ДокументОснование,

		0                           КАК ВНаличииДляПродажи,
		Т.ВРезервеОстаток           КАК Резерв,
		-Т.ВРезервеОстаток          КАК СвободноДляПродажи,
		Т.ВРезервеОстаток
			+ Т.КОтгрузкеОстаток    КАК ВНаличииНаСкладе,
		0                           КАК ОбособленныйРезерв,
		Т.КОтгрузкеОстаток          КАК Отгружается

	ИЗ
		РегистрНакопления.ТоварыКОтгрузке.Остатки( , {
			(Номенклатура, Характеристика) В(
				ВЫБРАТЬ
					ТаблицаОтбора.Номенклатура    КАК Номенклатура,
					ТаблицаОтбора.Характеристика  КАК Характеристика
				ИЗ
					ОтборПоСегментуНоменклатуры КАК ТаблицаОтбора
				ГДЕ
					ТаблицаОтбора.ИспользуетсяОтборПоСегментуНоменклатуры = &amp;ИспользуетсяОтборПоСегментуНоменклатуры)
			}	 {Склад.*, Номенклатура.*, Характеристика.*}) КАК Т

) КАК Набор

СГРУППИРОВАТЬ ПО
	Набор.Номенклатура,
	Набор.Характеристика,
	Набор.Склад,
	Набор.ДокументОснование

</query>
