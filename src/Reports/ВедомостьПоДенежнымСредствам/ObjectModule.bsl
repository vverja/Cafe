Процедура ПодключитьЗапрос(ДинамическийТекстЗапроса, ПодключаемыйЗапрос, ТипЗапроса = Неопределено, КоличествоПодключенныхЗапросов)
	Если Не ПустаяСтрока(ПодключаемыйЗапрос) Тогда
		Если КоличествоПодключенныхЗапросов = 0 Тогда
			ДинамическийТекстЗапроса = ДинамическийТекстЗапроса + ПодключаемыйЗапрос;
			
			Если ТипЗапроса <> Неопределено Тогда
				Если ТипЗапроса = "ОсновнаяАналитика" Тогда
					СтрокаВыбораПолейСКД = СтрокаВыбратьСКДОсновнаяАналитика();
				ИначеЕсли ТипЗапроса = "ОборотнаяАналитика" Тогда
					СтрокаВыбораПолейСКД = СтрокаВыбратьСКДОборотнаяАналитика();
				ИначеЕсли ТипЗапроса = "Документы" Тогда
					СтрокаВыбораПолейСКД = СтрокаВыбратьСКДДокументы();
				КонецЕсли;
				ДинамическийТекстЗапроса = СтрЗаменить(ДинамическийТекстЗапроса, "ИЗ" + Символы.ПС, СтрокаВыбораПолейСКД + Символы.ПС + "ИЗ" + Символы.ПС);
			КонецЕсли;
		Иначе
			ПодключаемыйЗапросПодготовка = СтрЗаменить(ПодключаемыйЗапрос, "РАЗРЕШЕННЫЕ", "");
			Если ТипЗапроса <> Неопределено Тогда
				Если ТипЗапроса = "ОсновнаяАналитика" Тогда
					СтрокаВременнойТаблицы = "ПОМЕСТИТЬ ОстаткиИОбороты";
				ИначеЕсли ТипЗапроса = "ОборотнаяАналитика" Тогда
					СтрокаВременнойТаблицы = "ПОМЕСТИТЬ ОборотнаяАналитика";
				КонецЕсли;
				ПодключаемыйЗапросПодготовка = СтрЗаменить(ПодключаемыйЗапросПодготовка, СтрокаВременнойТаблицы, "");
			КонецЕсли;
			
			ДинамическийТекстЗапроса = ДинамическийТекстЗапроса + Символы.ПС + Символы.ПС 
			+ "ОБЪЕДИНИТЬ ВСЕ" + Символы.ПС + Символы.ПС;
			
			ДинамическийТекстЗапроса = ДинамическийТекстЗапроса + ПодключаемыйЗапросПодготовка;
		КонецЕсли;
		
		КоличествоПодключенныхЗапросов = КоличествоПодключенныхЗапросов + 1;
	КонецЕсли;
КонецПроцедуры

Функция ПраваЧтения()
	ПраваЧтения = Новый Структура;
	
	// Проверим права чтения регистров
	ПраваЧтения.Вставить("ДенежныеСредстваБезналичные", 	ПравоДоступа("Чтение", Метаданные.РегистрыНакопления.ДенежныеСредстваБезналичные));
