Функция ИсполняемыйЗапросОсновнаяАналитика(ПраваЧтения)
	ТекстЗапроса = "";
	
	ТипЗапроса = "ОсновнаяАналитика";
	КоличествоПодключенныхЗапросов = 0;
	
	Если ПраваЧтения.ДенежныеСредстваБезналичные Тогда
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДенежныеСредстваБезналичныеНачальныйОстаток(), ТипЗапроса, КоличествоПодключенныхЗапросов);
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДенежныеСредстваБезналичныеОбороты(), ТипЗапроса, КоличествоПодключенныхЗапросов);
