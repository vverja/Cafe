	ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДвиженияДенежныхСредствПриход(), ТипЗапроса, КоличествоПодключенныхЗапросов);
	ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДвиженияДенежныхСредствРасход(), ТипЗапроса, КоличествоПодключенныхЗапросов);
	
	Если ПраваЧтения.ДенежныеСредстваБезналичные Тогда
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДополнениеВводОстатковДенежныеСредстваБезналичные(), ТипЗапроса, КоличествоПодключенныхЗапросов);
	КонецЕсли;
	Если ПраваЧтения.ДенежныеСредстваВКассахККМ Тогда
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДополнениеВводОстатковДенежныеСредстваВКассахККМ(), ТипЗапроса, КоличествоПодключенныхЗапросов);
	КонецЕсли;
	Если ПраваЧтения.ДенежныеСредстваВПути Тогда
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДополнениеВводОстатковДенежныеСредстваВПути(), ТипЗапроса, КоличествоПодключенныхЗапросов);
	КонецЕсли;
	Если ПраваЧтения.ДенежныеСредстваНаличные Тогда
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДополнениеВводОстатковДенежныеСредстваНаличные(), ТипЗапроса, КоличествоПодключенныхЗапросов);
	КонецЕсли;
	
	ТекстЗапроса = ТекстЗапроса + Символы.ПС + ";" + Символы.ПС;
	
	КоличествоПодключенныхЗапросов = 0;
	
	ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаКлассифицированныеОбороты(), , КоличествоПодключенныхЗапросов);
	ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаНеклассифицированныеОбороты(), , КоличествоПодключенныхЗапросов);
	
	Возврат ТекстЗапроса;
КонецФункции

