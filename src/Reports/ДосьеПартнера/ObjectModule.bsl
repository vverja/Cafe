			Область.Параметры.НомерДата = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(НСтр("ru='№ %1 от %2';uk='№ %1 від %2'"),
			                                                                                      ВыборкаДетали.Номер,
			                                                                                      ВыборкаДетали.Дата);
