	Партнер,
	ИспользуетсяОтборПоСегментуПартнеров
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ РАЗЛИЧНЫЕ
	Сегменты.Номенклатура КАК Номенклатура,
	Сегменты.Характеристика КАК Характеристика,
	ИСТИНА КАК ИспользуетсяОтборПоСегментуНоменклатуры
ПОМЕСТИТЬ ОтборПоСегментуНоменклатуры
ИЗ
	РегистрСведений.НоменклатураСегмента КАК Сегменты
{ГДЕ
	Сегменты.Сегмент.* КАК СегментНоменклатуры,
	Сегменты.Номенклатура.* КАК Номенклатура,
	Сегменты.Характеристика.* КАК Характеристика}

ИНДЕКСИРОВАТЬ ПО
	Номенклатура,
	Характеристика,
	ИспользуетсяОтборПоСегментуНоменклатуры
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ЕСТЬNULL(СебестоимостьТоваров.ВидЗапасов.Поставщик, ЗНАЧЕНИЕ(Справочник.Партнеры.ПустаяСсылка)) КАК Поставщик,
	ЕСТЬNULL(СебестоимостьТоваров.ВидЗапасов.Менеджер, ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)) КАК МенеджерПоЗакупкам,
	АналитикаНоменклатуры.Номенклатура КАК Номенклатура,
	СебестоимостьТоваров.Регистратор КАК ДокументЗакупки,
	НАЧАЛОПЕРИОДА(СебестоимостьТоваров.Период, ДЕНЬ) КАК ПериодДеньБаза,
	НАЧАЛОПЕРИОДА(СебестоимостьТоваров.Период, НЕДЕЛЯ) КАК ПериодНеделяБаза,
	НАЧАЛОПЕРИОДА(СебестоимостьТоваров.Период, ДЕКАДА) КАК ПериодДекадаБаза,
	НАЧАЛОПЕРИОДА(СебестоимостьТоваров.Период, МЕСЯЦ) КАК ПериодМесяцБаза,
	НАЧАЛОПЕРИОДА(СебестоимостьТоваров.Период, КВАРТАЛ) КАК ПериодКварталБаза,
	НАЧАЛОПЕРИОДА(СебестоимостьТоваров.Период, ПОЛУГОДИЕ) КАК ПериодПолугодиеБаза,
	НАЧАЛОПЕРИОДА(СебестоимостьТоваров.Период, ГОД) КАК ПериодГодБаза,
	СУММА(СебестоимостьТоваров.Количество) КАК КоличествоЗакупкиБаза,
	СУММА(СебестоимостьТоваров.Стоимость) КАК СтоимостьЗакупкиБаза
ПОМЕСТИТЬ ЗакупкиБазовыйПериод
{ВЫБРАТЬ
	Поставщик.*,
	МенеджерПоЗакупкам.*,
	Номенклатура.*,
	ДокументЗакупки.*,
	ПериодДеньБаза,
	ПериодНеделяБаза,
	ПериодДекадаБаза,
	ПериодМесяцБаза,
	ПериодКварталБаза,
	ПериодПолугодиеБаза,
	ПериодГодБаза,
	КоличествоЗакупкиБаза,
	СтоимостьЗакупкиБаза}
ИЗ
	РегистрНакопления.СебестоимостьТоваров КАК СебестоимостьТоваров
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.АналитикаУчетаНоменклатуры КАК АналитикаНоменклатуры
		ПО СебестоимостьТоваров.АналитикаУчетаНоменклатуры = АналитикаНоменклатуры.КлючАналитики}
		{ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.АналитикаУчетаПоПартнерам КАК АналитикаПартнеров
		ПО СебестоимостьТоваров.АналитикаУчетаПоПартнерам = АналитикаПартнеров.КлючАналитики}
ГДЕ
	СебестоимостьТоваров.Период МЕЖДУ &amp;НачалоПериода И &amp;ОкончаниеБазовогоПериода
	И (СебестоимостьТоваров.ХозяйственнаяОперация = ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ЗакупкаУПоставщика)
			ИЛИ СебестоимостьТоваров.ХозяйственнаяОперация = ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ЗакупкаПоИмпорту)
			ИЛИ СебестоимостьТоваров.ХозяйственнаяОперация = ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ЗакупкаЧерезПодотчетноеЛицо))
{ГДЕ
	(АналитикаНоменклатуры.Номенклатура В
			(ВЫБРАТЬ
				ОтборПоСегментуНоменклатуры.Номенклатура
			ИЗ
				ОтборПоСегментуНоменклатуры
			ГДЕ
				ОтборПоСегментуНоменклатуры.ИспользуетсяОтборПоСегментуНоменклатуры = &amp;ИспользуетсяОтборПоСегментуНоменклатуры)),
	(АналитикаПартнеров.Партнер В
			(ВЫБРАТЬ
				ОтборПоСегментуПартнеров.Партнер
			ИЗ
				ОтборПоСегментуПартнеров
			ГДЕ
				ОтборПоСегментуПартнеров.ИспользуетсяОтборПоСегментуПартнеров = &amp;ИспользуетсяОтборПоСегментуПартнеров))}

СГРУППИРОВАТЬ ПО
	СебестоимостьТоваров.ВидЗапасов.Поставщик,
	СебестоимостьТоваров.ВидЗапасов.Менеджер,
	АналитикаНоменклатуры.Номенклатура,
	СебестоимостьТоваров.Регистратор,
