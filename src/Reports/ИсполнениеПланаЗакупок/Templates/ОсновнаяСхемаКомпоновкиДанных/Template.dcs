	СУММА(ЗакупкиОбороты.КоличествоОборот) КАК ФактКоличество,
	СУММА(ВЫРАЗИТЬ(ЗакупкиОбороты.СуммаОборот * ВЫБОР
				КОГДА &amp;ВалютаУпр &lt;&gt; &amp;ВалютаСценария
					ТОГДА ВЫБОР
							КОГДА ЕСТЬNULL(КурсыВалютыУпр.Кратность, ЕСТЬNULL(КурсыВалютыУпрНаНачало.Кратность, 0)) &gt; 0
									И ЕСТЬNULL(КурсыВалютыУпр.Курс, ЕСТЬNULL(КурсыВалютыУпрНаНачало.Курс, 0)) &gt; 0
									И ЕСТЬNULL(КурсыВалюты.Кратность, ЕСТЬNULL(КурсыВалютыНаНачало.Кратность, 0)) &gt; 0
									И ЕСТЬNULL(КурсыВалюты.Курс, ЕСТЬNULL(КурсыВалютыНаНачало.Курс, 0)) &gt; 0
								ТОГДА ЕСТЬNULL(КурсыВалютыУпр.Курс, ЕСТЬNULL(КурсыВалютыУпрНаНачало.Курс, 0)) * ЕСТЬNULL(КурсыВалюты.Кратность, ЕСТЬNULL(КурсыВалютыНаНачало.Кратность, 0)) / (ЕСТЬNULL(КурсыВалюты.Курс, ЕСТЬNULL(КурсыВалютыНаНачало.Курс, 0)) * ЕСТЬNULL(КурсыВалютыУпр.Кратность, ЕСТЬNULL(КурсыВалютыУпрНаНачало.Кратность, 0)))
							ИНАЧЕ 0
						КОНЕЦ
				ИНАЧЕ 1
			КОНЕЦ КАК ЧИСЛО(15, 2))) КАК ФактСумма
