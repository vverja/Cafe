	ДенежныеСредства.ПериодГод,
	(ВЫБОР
		КОГДА &amp;ДанныеПоДенежнымСредствам = 1
			ТОГДА ДенежныеСредства.СуммаНачальныйОстаток * (ЕСТЬNULL(КурсВалютыОтправитель.Курс, 1) / ЕСТЬNULL(КурсВалютыОтправитель.Кратность, 1)) * (ЕСТЬNULL(КурсВалютыПолучатель.Кратность, 1) / ЕСТЬNULL(КурсВалютыПолучатель.Курс, 1))
		ИНАЧЕ ДенежныеСредства.СуммаНачальныйОстаток * (ЕСТЬNULL(КурсВалютыОтправитель.Курс, 1) / ЕСТЬNULL(КурсВалютыОтправитель.Кратность, 1)) * (ЕСТЬNULL(КурсВалютыОтчета.Кратность, 1) / ЕСТЬNULL(КурсВалютыОтчета.Курс, 1))
	КОНЕЦ) КАК СуммаКПоступлениюНачальныйОстаток,
	(ВЫБОР
		КОГДА &amp;ДанныеПоДенежнымСредствам = 1
			ТОГДА ДенежныеСредства.СуммаПриход * (ЕСТЬNULL(КурсВалютыОтправитель.Курс, 1) / ЕСТЬNULL(КурсВалютыОтправитель.Кратность, 1)) * (ЕСТЬNULL(КурсВалютыПолучатель.Кратность, 1) / ЕСТЬNULL(КурсВалютыПолучатель.Курс, 1))
		ИНАЧЕ ДенежныеСредства.СуммаПриход * (ЕСТЬNULL(КурсВалютыОтправитель.Курс, 1) / ЕСТЬNULL(КурсВалютыОтправитель.Кратность, 1)) * (ЕСТЬNULL(КурсВалютыОтчета.Кратность, 1) / ЕСТЬNULL(КурсВалютыОтчета.Курс, 1))
