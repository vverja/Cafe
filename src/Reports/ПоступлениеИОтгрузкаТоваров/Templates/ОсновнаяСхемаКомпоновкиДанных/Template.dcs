		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ РАЗЛИЧНЫЕ
	Сегменты.Номенклатура КАК Номенклатура,
	Сегменты.Характеристика КАК Характеристика,
	ИСТИНА КАК ИспользуетсяОтборПоСегментуНоменклатуры
ПОМЕСТИТЬ ОтборПоСегментуНоменклатуры
ИЗ
	РегистрСведений.НоменклатураСегмента КАК Сегменты
{ГДЕ
	Сегменты.Сегмент.* КАК СегментНоменклатуры,
	Сегменты.Номенклатура.* КАК Номенклатура,
	Сегменты.Характеристика.* КАК Характеристика}

ИНДЕКСИРОВАТЬ ПО
	Номенклатура,
	Характеристика,
	ИспользуетсяОтборПоСегментуНоменклатуры
;

//Расчет доступного количества для товаров, имеющих просроченные движения
ВЫБРАТЬ РАЗРЕШЕННЫЕ РАЗЛИЧНЫЕ
	Набор.Номенклатура   КАК Номенклатура,
	Набор.Характеристика КАК Характеристика,
	Набор.Склад          КАК Склад
ПОМЕСТИТЬ ВтТоварыДляРасчетаДоступности
ИЗ(
	ВЫБРАТЬ
		Таблица.Номенклатура   КАК Номенклатура,
		Таблица.Характеристика КАК Характеристика,
		Таблица.Склад          КАК Склад
	ИЗ
		РегистрНакопления.ГрафикПоступленияТоваров.Остатки(, {Номенклатура.*, Характеристика.*, Склад.*}) КАК Таблица
	ГДЕ
		Таблица.ДатаСобытия &lt; &amp;НачалоТекущегоДня
		И Таблица.КоличествоИзЗаказовОстаток &lt;&gt; 0

	ОБЪЕДИНИТЬ ВСЕ

	ВЫБРАТЬ
		Таблица.Номенклатура   КАК Номенклатура,
		Таблица.Характеристика КАК Характеристика,
		Таблица.Склад          КАК Склад
	ИЗ
		РегистрНакопления.ГрафикОтгрузкиТоваров.Остатки(, {Номенклатура.*, Характеристика.*, Склад.*}) КАК Таблица
	ГДЕ
		Таблица.ДатаОтгрузки &lt; &amp;НачалоТекущегоДня
		И Таблица.КоличествоИзЗаказовОстаток &lt;&gt; 0) КАК Набор

ИНДЕКСИРОВАТЬ ПО
	Номенклатура, Характеристика, Склад
;

ВЫБРАТЬ РАЗЛИЧНЫЕ
	Набор.Номенклатура   КАК Номенклатура,
	Набор.Характеристика КАК Характеристика,
	Набор.Склад          КАК Склад,
	Набор.Период         КАК Период
ПОМЕСТИТЬ ВтПериодыДвижений
ИЗ(
	ВЫБРАТЬ
		Таблица.Номенклатура   КАК Номенклатура,
		Таблица.Характеристика КАК Характеристика,
		Таблица.Склад          КАК Склад,

		ВЫБОР КОГДА График.ДатаСобытия &lt; &amp;НачалоТекущегоДня ТОГДА
					&amp;НачалоТекущегоДня
				ИНАЧЕ
					График.ДатаСобытия
			КОНЕЦ              КАК Период

	ИЗ
		ВтТоварыДляРасчетаДоступности КАК Таблица

			ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрНакопления.ГрафикПоступленияТоваров.Остатки КАК График
			ПО Таблица.Номенклатура   = График.Номенклатура
			 И Таблица.Характеристика = График.Характеристика
			 И Таблица.Склад          = График.Склад
			 И График.КоличествоИзЗаказовОстаток &lt;&gt; 0

	ОБЪЕДИНИТЬ ВСЕ

	ВЫБРАТЬ
		Таблица.Номенклатура   КАК Номенклатура,
		Таблица.Характеристика КАК Характеристика,
		Таблица.Склад          КАК Склад,

		ВЫБОР КОГДА График.ДатаОтгрузки &lt; &amp;НачалоТекущегоДня ТОГДА
					&amp;НачалоТекущегоДня
				ИНАЧЕ
					График.ДатаОтгрузки
			КОНЕЦ              КАК Период

	ИЗ
		ВтТоварыДляРасчетаДоступности КАК Таблица

			ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрНакопления.ГрафикОтгрузкиТоваров.Остатки КАК График
			ПО Таблица.Номенклатура   = График.Номенклатура
			 И Таблица.Характеристика = График.Характеристика
			 И Таблица.Склад          = График.Склад
			 И График.КоличествоИзЗаказовОстаток &lt;&gt; 0) КАК Набор
;

ВЫБРАТЬ
	Таблица.Номенклатура             КАК Номенклатура,
	Таблица.Характеристика           КАК Характеристика,
	Таблица.Склад                    КАК Склад,
	МИНИМУМ(Таблица.КонечныйОстаток) КАК МинимальныйОстаток
ПОМЕСТИТЬ ВтМинимальныйОстаток
ИЗ(
	ВЫБРАТЬ
		Таблица.Номенклатура   КАК Номенклатура,
		Таблица.Характеристика КАК Характеристика,
		Таблица.Склад          КАК Склад,

		Таблица.Период         КАК Период,

		СУММА(Таблица.КонечныйОстаток) КАК КонечныйОстаток
	ИЗ(
		ВЫБРАТЬ
			Таблица.Номенклатура   КАК Номенклатура,
			Таблица.Характеристика КАК Характеристика,
			Таблица.Склад          КАК Склад,

			Таблица.Период         КАК Период,

			ЕСТЬNULL(ГрафикПоступления.КоличествоИзЗаказовОстаток, 0) КАК КонечныйОстаток

		ИЗ
			ВтПериодыДвижений КАК Таблица

				ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ГрафикПоступленияТоваров.Остатки КАК ГрафикПоступления
				ПО Таблица.Номенклатура   = ГрафикПоступления.Номенклатура
				 И Таблица.Характеристика = ГрафикПоступления.Характеристика
				 И Таблица.Склад          = ГрафикПоступления.Склад
				 И ГрафикПоступления.КоличествоИзЗаказовОстаток &lt;&gt; 0

				 И Таблица.Период &gt;= ГрафикПоступления.ДатаСобытия

		ОБЪЕДИНИТЬ ВСЕ

		ВЫБРАТЬ
			Таблица.Номенклатура   КАК Номенклатура,
			Таблица.Характеристика КАК Характеристика,
			Таблица.Склад          КАК Склад,

			Таблица.Период         КАК Период,

			- ЕСТЬNULL(ГрафикОтгрузки.КоличествоИзЗаказовОстаток, 0) КАК КонечныйОстаток

		ИЗ
			ВтПериодыДвижений КАК Таблица

				ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ГрафикОтгрузкиТоваров.Остатки КАК ГрафикОтгрузки
				ПО Таблица.Номенклатура   = ГрафикОтгрузки.Номенклатура
				 И Таблица.Характеристика = ГрафикОтгрузки.Характеристика
				 И Таблица.Склад          = ГрафикОтгрузки.Склад
				 И ГрафикОтгрузки.КоличествоИзЗаказовОстаток &lt;&gt; 0

				 И Таблица.Период &gt;= ГрафикОтгрузки.ДатаОтгрузки) КАК Таблица

		СГРУППИРОВАТЬ ПО
			Таблица.Номенклатура, Таблица.Характеристика, Таблица.Склад, Таблица.Период
	) КАК Таблица

СГРУППИРОВАТЬ ПО
	Таблица.Номенклатура, Таблица.Характеристика, Таблица.Склад
ИНДЕКСИРОВАТЬ ПО
	Таблица.Номенклатура, Таблица.Характеристика, Таблица.Склад
;

ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Таблица.Номенклатура             КАК Номенклатура,
	Таблица.Характеристика           КАК Характеристика,
	Таблица.Склад                    КАК Склад,

	Таблица.ДатаДоступности          КАК ДатаДоступности,
	Таблица.Количество               КАК Количество
ПОМЕСТИТЬ ВтДоступноВГрафике
ИЗ
	РегистрСведений.ДоступныеОстаткиПланируемыхПоступлений КАК Таблица
		ЛЕВОЕ СОЕДИНЕНИЕ ВтТоварыДляРасчетаДоступности КАК Фильтр
		ПО Таблица.Номенклатура   = Фильтр.Номенклатура
		 И Таблица.Характеристика = Фильтр.Характеристика
		 И Таблица.Склад          = Фильтр.Склад
ГДЕ
	Фильтр.Номенклатура ЕСТЬ NULL
	ИЛИ Таблица.ДатаДоступности &gt; &amp;НачалоТекущегоДня

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Таблица.Номенклатура             КАК Номенклатура,
	Таблица.Характеристика           КАК Характеристика,
	Таблица.Склад                    КАК Склад,

	ДАТАВРЕМЯ(1, 1, 1)               КАК ПериодРасшифровка,
	Таблица.МинимальныйОстаток       КАК Доступно

ИЗ
	ВтМинимальныйОстаток КАК Таблица
ГДЕ
	Таблица.МинимальныйОстаток &lt; 0

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Таблица.Номенклатура             КАК Номенклатура,
	Таблица.Характеристика           КАК Характеристика,
	Таблица.Склад                    КАК Склад,

	&amp;НачалоТекущегоДня               КАК ПериодРасшифровка,
	Таблица.МинимальныйОстаток       КАК Доступно

ИЗ
	ВтМинимальныйОстаток КАК Таблица
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ДоступныеОстаткиПланируемыхПоступлений КАК Фильтр
		ПО Таблица.Номенклатура   = Фильтр.Номенклатура
		 И Таблица.Характеристика = Фильтр.Характеристика
		 И Таблица.Склад          = Фильтр.Склад

		 И Фильтр.ДатаДоступности &gt; &amp;НачалоТекущегоДня
		 И Таблица.МинимальныйОстаток = Фильтр.Количество

ГДЕ
	Таблица.МинимальныйОстаток &gt; 0
	И Фильтр.Номенклатура ЕСТЬ NULL
;

ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Набор.Номенклатура             КАК Номенклатура,
	Набор.Характеристика           КАК Характеристика,
	Набор.Склад                    КАК Склад,

	Набор.ПериодРасшифровка        КАК ПериодРасшифровка,
	СУММА(Набор.Доступно)          КАК Доступно

ИЗ(
	ВЫБРАТЬ
		Таблица.Номенклатура             КАК Номенклатура,
		Таблица.Характеристика           КАК Характеристика,
		Таблица.Склад                    КАК Склад,

		Таблица.ДатаДоступности          КАК ПериодРасшифровка,
		Таблица.Количество               КАК Доступно
	ИЗ
		ВтДоступноВГрафике КАК Таблица
		
	ОБЪЕДИНИТЬ ВСЕ

	ВЫБРАТЬ
		Таблица.Номенклатура             КАК Номенклатура,
		Таблица.Характеристика           КАК Характеристика,
		Таблица.Склад                    КАК Склад,

		Таблица.ДатаДоступности          КАК ПериодРасшифровка,
		Остатки.ВНаличииОстаток
			- Остатки.ВРезервеСоСкладаОстаток
			- Остатки.ВРезервеПодЗаказОстаток   КАК Доступно
	ИЗ
		ВтДоступноВГрафике КАК Таблица
			ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрНакопления.СвободныеОстатки.Остатки КАК Остатки
			ПО Таблица.Номенклатура   = Остатки.Номенклатура
			 И Таблица.Характеристика = Остатки.Характеристика
			 И Таблица.Склад          = Остатки.Склад
			 И Таблица.ДатаДоступности &gt; ДАТАВРЕМЯ(1, 1, 1)
		
	ОБЪЕДИНИТЬ ВСЕ

	ВЫБРАТЬ
		Таблица.Номенклатура             КАК Номенклатура,
		Таблица.Характеристика           КАК Характеристика,
		Таблица.Склад                    КАК Склад,

		ДАТАВРЕМЯ(1, 1, 1)               КАК ПериодРасшифровка,
		Таблица.ВНаличииОстаток
			- Таблица.ВРезервеСоСкладаОстаток
			- Таблица.ВРезервеПодЗаказОстаток КАК Доступно
	ИЗ
		РегистрНакопления.СвободныеОстатки.Остатки КАК Таблица) КАК Набор
СГРУППИРОВАТЬ ПО
	Набор.Номенклатура, Набор.Характеристика, Набор.Склад,
	Набор.ПериодРасшифровка

</query>
