		<dataSource>ИсточникДанных1</dataSource>
		<query>// 1) В наличии
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	0                                             КАК Действие,
	Таблица.Номенклатура                          КАК Номенклатура,
	Таблица.Характеристика                        КАК Характеристика,
	Таблица.Склад                                 КАК Склад,

	ДАТАВРЕМЯ(1, 1, 1)                            КАК ПериодРасшифровка,
	&amp;ВНаличии                                     КАК ДокументОснование,
	ДАТАВРЕМЯ(1, 1, 1)                            КАК ДатаДокумента,

	Таблица.ВНаличииОстаток                       КАК НачальныйОстаток,
	0                                             КАК Приход,
	0                                             КАК Расход

ИЗ
	РегистрНакопления.СвободныеОстатки.Остатки КАК Таблица

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	0                        КАК Действие,
	Таблица.Номенклатура     КАК Номенклатура,
	Таблица.Характеристика   КАК Характеристика,
	Таблица.Склад            КАК Склад,

	ДАТАВРЕМЯ(1, 1, 1)       КАК ПериодРасшифровка,
	&amp;ВНаличии                КАК ДокументОснование,
	ДАТАВРЕМЯ(1, 1, 1)       КАК ДатаДокумента,

	Таблица.КОтгрузкеОстаток КАК НачальныйОстаток,
	0                        КАК Приход,
	0                        КАК Расход
ИЗ
	РегистрНакопления.ТоварыКОтгрузке.Остатки КАК Таблица

	ОБЪЕДИНИТЬ ВСЕ

// 2) Отгружается
ВЫБРАТЬ
	1                                             КАК Действие,
	Таблица.Номенклатура                          КАК Номенклатура,
	Таблица.Характеристика                        КАК Характеристика,
	Таблица.Склад                                 КАК Склад,

	ДАТАВРЕМЯ(1, 1, 1)                            КАК ПериодРасшифровка,
	Таблица.ДокументОтгрузки                      КАК ДокументОснование,
	Таблица.ДокументОтгрузки.Дата                 КАК ДатаДокумента,

	0                                             КАК НачальныйОстаток,
	0                                             КАК Приход,
	Таблица.КОтгрузкеОстаток                      КАК Расход

ИЗ
	РегистрНакопления.ТоварыКОтгрузке.Остатки КАК Таблица

	ОБЪЕДИНИТЬ ВСЕ

// 3) Резервировать под заказ
ВЫБРАТЬ
	2                                             КАК Действие,
	Таблица.Номенклатура                          КАК Номенклатура,
	Таблица.Характеристика                        КАК Характеристика,
	Таблица.Склад                                 КАК Склад,

	ДАТАВРЕМЯ(1, 1, 1)                            КАК ПериодРасшифровка,

	ВЫБОР КОГДА Таблица.Назначение.Заказ = НЕОПРЕДЕЛЕНО ТОГДА
					Таблица.Назначение
				ИНАЧЕ
					Таблица.Назначение.Заказ
		КОНЕЦ                                     КАК ДокументОснование,
		
	ВЫБОР КОГДА Таблица.Назначение.Заказ = НЕОПРЕДЕЛЕНО ТОГДА
					ДАТАВРЕМЯ(1, 1, 1)
				ИНАЧЕ
					Таблица.Назначение.Заказ.Дата
		КОНЕЦ                                     КАК ДатаДокумента,

	0                                             КАК НачальныйОстаток,
	0                                             КАК Приход,
	Таблица.НаличиеПодЗаказОстаток                КАК Расход

ИЗ
	РегистрНакопления.ОбеспечениеЗаказов.Остатки КАК Таблица

ОБЪЕДИНИТЬ ВСЕ

// 4) Резервировать на складе
ВЫБРАТЬ
	3                                             КАК Действие,
	Таблица.Номенклатура                          КАК Номенклатура,
	Таблица.Характеристика                        КАК Характеристика,
	Таблица.Склад                                 КАК Склад,

	ДАТАВРЕМЯ(1, 1, 1)                            КАК ПериодРасшифровка,
	Таблица.ДокументОтгрузки                      КАК ДокументОснование,
	Таблица.ДокументОтгрузки.Дата                 КАК ДатаДокумента,

	0                                             КАК НачальныйОстаток,
	0                                             КАК Приход,
	Таблица.ВРезервеОстаток                       КАК Расход

ИЗ
	РегистрНакопления.ТоварыКОтгрузке.Остатки КАК Таблица

ОБЪЕДИНИТЬ ВСЕ

// 5) Ожидается к поступлению
ВЫБРАТЬ
	4                                             КАК Действие,
	Таблица.Номенклатура                          КАК Номенклатура,
	Таблица.Характеристика                        КАК Характеристика,
	Таблица.Склад                                 КАК Склад,

	ВЫБОР КОГДА Таблица.ДатаСобытия &lt; &amp;НачалоТекущегоДня ТОГДА
				&amp;НачалоТекущегоДня
			ИНАЧЕ
				Таблица.ДатаСобытия
		КОНЕЦ                                     КАК ПериодРасшифровка,

	Таблица.Регистратор                           КАК ДокументОснование,
	МАКСИМУМ(Таблица.Регистратор.Дата)            КАК ДатаДокумента,

	0                                             КАК НачальныйОстаток,
	СУММА(Таблица.КоличествоИзЗаказов)            КАК Приход,
	0                                             КАК Расход

ИЗ
	РегистрНакопления.ГрафикПоступленияТоваров КАК Таблица

ГДЕ
	Таблица.КоличествоИзЗаказов &lt;&gt; 0

СГРУППИРОВАТЬ ПО
	Таблица.Номенклатура, Таблица.Характеристика, Таблица.Склад,
	ВЫБОР КОГДА Таблица.ДатаСобытия &lt; &amp;НачалоТекущегоДня ТОГДА
				&amp;НачалоТекущегоДня
			ИНАЧЕ
				Таблица.ДатаСобытия
		КОНЕЦ,
	Таблица.Регистратор

ОБЪЕДИНИТЬ ВСЕ

// 6) Резервировать к дате
ВЫБРАТЬ
	5                                             КАК Действие,
	Таблица.Номенклатура                          КАК Номенклатура,
	Таблица.Характеристика                        КАК Характеристика,
	Таблица.Склад                                 КАК Склад,

	ВЫБОР КОГДА Таблица.ДатаОтгрузки &lt; &amp;НачалоТекущегоДня ТОГДА
				&amp;НачалоТекущегоДня
			ИНАЧЕ
				Таблица.ДатаОтгрузки
		КОНЕЦ                                     КАК ПериодРасшифровка,

	Таблица.Регистратор                           КАК ДокументОснование,
	МАКСИМУМ(Таблица.Регистратор.Дата)            КАК ДатаДокумента,

	0                                             КАК НачальныйОстаток,
	0                                             КАК Приход,
	СУММА(Таблица.КоличествоИзЗаказов)            КАК Расход

ИЗ
	РегистрНакопления.ГрафикОтгрузкиТоваров КАК Таблица

ГДЕ
	Таблица.КоличествоИзЗаказов &lt;&gt; 0

СГРУППИРОВАТЬ ПО
	Таблица.Номенклатура, Таблица.Характеристика, Таблица.Склад,
	ВЫБОР КОГДА Таблица.ДатаОтгрузки &lt; &amp;НачалоТекущегоДня ТОГДА
				&amp;НачалоТекущегоДня
			ИНАЧЕ
				Таблица.ДатаОтгрузки
		КОНЕЦ,
	Таблица.Регистратор


</query>
