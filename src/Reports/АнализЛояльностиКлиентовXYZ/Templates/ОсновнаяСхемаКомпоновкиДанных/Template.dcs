				</dcsset:item>
				<dcsset:viewMode>Normal</dcsset:viewMode>
				<dcsset:userSettingID>751b4330-ea53-4a78-921b-75f352d53003</dcsset:userSettingID>
			</dcsset:selection>
			<dcsset:filter>
