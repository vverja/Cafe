	КонецЕсли;
	Если ПраваЧтения.ДенежныеСредстваВКассахККМ Тогда
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДенежныеСредстваВКассахККМНачальныйОстаток(), ТипЗапроса, КоличествоПодключенныхЗапросов);
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДенежныеСредстваВКассахККМОбороты(), ТипЗапроса, КоличествоПодключенныхЗапросов);
