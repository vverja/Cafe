	КонецЕсли;
	Если ПраваЧтения.ДенежныеСредстваВПути Тогда
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДенежныеСредстваВПутиНачальныйОстаток(), ТипЗапроса, КоличествоПодключенныхЗапросов);
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДенежныеСредстваВПутиОбороты(), ТипЗапроса, КоличествоПодключенныхЗапросов);
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДенежныеСредстваВПутиКонечныйОстаток(), ТипЗапроса, КоличествоПодключенныхЗапросов);
	КонецЕсли;
	Если ПраваЧтения.ДенежныеСредстваНаличные Тогда
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДенежныеСредстваНаличныеНачальныйОстаток(), ТипЗапроса, КоличествоПодключенныхЗапросов);
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДенежныеСредстваНаличныеОбороты(), ТипЗапроса, КоличествоПодключенныхЗапросов);
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДенежныеСредстваНаличныеКонечныйОстаток(), ТипЗапроса, КоличествоПодключенныхЗапросов);
	КонецЕсли;
	Если ПраваЧтения.ДенежныеСредстваУПодотчетныхЛиц Тогда
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДенежныеСредстваУПодотчетныхЛицНачальныйОстаток(), ТипЗапроса, КоличествоПодключенныхЗапросов);
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДенежныеСредстваУПодотчетныхЛицОбороты(), ТипЗапроса, КоличествоПодключенныхЗапросов);
		ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДенежныеСредстваУПодотчетныхЛицКонечныйОстаток(), ТипЗапроса, КоличествоПодключенныхЗапросов);
	КонецЕсли;
	
	ТекстЗапроса = ТекстЗапроса + 
		ТекстЗапросаОстаткиИОбороты();
	
	Возврат ТекстЗапроса;
КонецФункции

Функция ИсполняемыйЗапросОборотнаяАналитика(ПраваЧтения)
	ТекстЗапроса = "";
	
	ТипЗапроса = "ОборотнаяАналитика";
	КоличествоПодключенныхЗапросов = 0;
	
	ПодключитьЗапрос(ТекстЗапроса, ТекстЗапросаДвиженияДенежныеСредстваДоходыРасходы(), ТипЗапроса, КоличествоПодключенныхЗапросов);
